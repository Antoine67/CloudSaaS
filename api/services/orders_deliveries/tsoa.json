{
    "entryFile": "./src/app.ts",
    "swagger": {
        "outputDirectory": ".",
        "entryFile": "./src/app.ts",
        "basePath": "/api",
        "host": "localhost:7000",
        "schemes": ["http"],
        "securityDefinitions": {
            "jwt": {
                "type": "apiKey",
                "name": "x-access-token",
                "in": "header"
            }
        }
    },
    "spec": {
        "outputDirectory": ".",
        "entryFile": "./src/app.ts",
        "basePath": "/api",
        "host": "localhost:7000",
        "schemes": ["http"],
        "securityDefinitions": {
            "jwt": {
                "type": "apiKey",
                "name": "x-access-token",
                "in": "header"
            }
        }
        
    },
    "routes": {
        "basePath": "/api",
        "entryFile": "./src/app.ts",
        "routesDir": "./src",
        "authenticationModule": "./src/authentication.ts"
    }
}